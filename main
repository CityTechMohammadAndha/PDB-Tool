#!/usr/bin/env python3
  
import sys


#atom_info = [serial_number, residue, chain_ID, res_sequence_num, x_coordinate, y_coordinate, z_coordinate, element_name]

def atom_info(count):
    file = open(sys.argv[1], "r")
    lines = file.readlines()
    atom_records = []
    for line in lines:
        if line.startswith('ATOM'):
            count += 1
            atom_records.append(line)
            print(atom_records)
           #print(line)
    return count

count = 0
frequency = atom_info(count)
print("Welcome to the pdb program. \nTo begin, try typing 'help' for the list of valid commands.")
print(frequency, "atoms recorded.")

x = input("Enter a command: ")





